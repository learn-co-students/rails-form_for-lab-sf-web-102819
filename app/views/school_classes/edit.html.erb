<h1>School Class Form</h1>

<%= form_for @class do |f| %>
  <%#= f.label :first_name %>
  <label>School Class Title:</label> 
  <%= f.text_field :title %>
  <br>

  <%#= f.label :last_name %>
  <label>School Class Room Number:</label> 
  <%= f.text_field :room_number %>
  <br>
  <%= f.submit %>
<% end %>